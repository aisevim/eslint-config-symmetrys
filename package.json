{
  "name": "eslint-config-symmetrys",
  "version": "0.8.0",
  "description": "Eslint Config",
  "license": "MIT",
  "author": "aisevim <a.aydnlp.pro@gmail.com>",
  "volta": {
    "node": "20.10.0"
  },
  "packageManager": "pnpm@8.10.5",
  "bugs": {
    "url": "https://github.com/aisevim/eslint-config-symmetrys/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aisevim/eslint-config-symmetrys.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-symmetry",
    "eslint-config-symmetrys",
    "symmetrys",
    "eslint-symmetrys"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.js --watch --format esm,cjs",
    "build": "tsup src/index.js --format esm,cjs --minify --clean",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . --ignore-pattern 'fixtures/' --rule 'security/detect-bidi-characters: off'",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "test:ci": "pnpm run build ; pnpm i ; vitest run",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:ci:test": "release-it --ci --dry-run",
    "install:ci": "pnpm i ; pnpm run build ; pnpm i"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vitest": "^0.3.15",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "lodash.assign": "^4.2.0",
    "lodash.mergewith": "^4.6.2",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/eslint": "^8.44.8",
    "eslint": "^8.55.0",
    "glob": "^10.3.10",
    "release-it": "^17.0.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "eslint": ">=8.54.0"
  }
}
